package collection;
import java.util.*;
public class myarray {

	public static void main(String[] args) {
		ArrayList<String> colour = new ArrayList<String>();
		 colour.add("blue");
		 colour.add("green");
		 colour.add("pink");
		 colour.add("yellow");
		 colour.add("orange");
		 colour.add("grey");
		 colour.add("violet");
		 System.out.println(colour.get(2));
	}

}
