package indicatearry;

public class jnj {
	public static void main(String[] args) {
		int arry[] = {1,2,3,4,5};
		int tofind=4;
	    boolean found = false;
 for(int n: arry) {
	if(n == tofind){
		found = true;
		break;
	}
 }
	if(found) {System.out.println(tofind + "  is found");}
	else{
		System.out.println(tofind + "  is not found");	
	}
}
}
